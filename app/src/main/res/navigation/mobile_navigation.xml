<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_dashboard"
        android:name="org.rmj.guanzon.guanzonsaleskit.Fragment.FragmentHome"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <activity
        android:id="@+id/nav_enrollagent"
        android:name="org.rmj.guanzongroup.agent.Activities.Activity_AgentEnroll"
        android:label="Enroll Agent"
        tools:layout="@layout/activity_agent_enroll"/>

    <activity
        android:id="@+id/nav_agentlist"
        android:name="org.rmj.guanzongroup.agent.Activities.Activity_AgentList"
        android:label="Agent List"
        tools:layout="@layout/activity_agent_list"/>

    <activity
        android:id="@+id/nav_profile"
        android:name="org.rmj.guanzongroup.authlibrary.Activity.Activity_Settings"
        android:label="Profile Settings"
        tools:layout="@layout/activity_settings"/>

    <activity
        android:id="@+id/nav_signout"
        android:name="org.rmj.guanzongroup.authlibrary.Activity.Activity_Login"
        android:label="Signout"
        tools:layout="@layout/activity_login"/>
</navigation>